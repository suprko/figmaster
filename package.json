{
  "name": "figmaster",
  "version": "0.0.0",
  "main": "dist/index.js",
  "repository": "git@github.com:suprko/figmaster.git",
  "author": "Lucas Huang <hello@superko.org>",
  "license": "GPL-3.0-or-later",
  "config": {
    "figmasterModulePath": "dist",
    "figmasterSource": ".figmaster"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "figmaster:saveFigmaJson": "node $npm_package_config_figmasterModulePath/commands/saveFigmaJson.js $(cat $npm_package_config_figmasterSource/token) $(cat $npm_package_config_figmasterSource/fileId)"
  },
  "devDependencies": {
    "@types/node": "^10.12.19",
    "typescript": "^3.2.4"
  }
}
